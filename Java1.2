package com.company;

public class Main
{
    public static void main(String[] args)
    {
         class Rectangle
         {
// Attributes
             private double ilgis = 10;
             private double plotis = 5;
             private double koordinate = 6;
 // Getter
             private double getIlgis() {
                return ilgis;
            }

             private double getPlotis() {
                return plotis;
            }

             private double getKoordinateY() {
                return ilgis;
            }
 // Setter
             public void setIlgis(double newIlgis) {
                 this.ilgis = newIlgis;
             }

             public void setPlotis (double newPlotis) {
                 this.plotis = newPlotis;
             }

             public void setKoordinateY (double newKoordinateY) {
                 this.koordinate = newKoordinateY;
             }
// Methods
             private void getPlotas() {
                System.out.println("Stačiakampio plotas: " + (ilgis*plotis)); }

             private void getPerimetras() {
                System.out.println("Stačiakampio perimetras: " + (2 * ilgis + 2 * plotis)); }

             private void getIstrizaine() {
                System.out.println("Įstrižainės ilgis: " + "%.2f", Math.sqrt(ilgis * ilgis + plotis * plotis));
               // System.out.println("");}

             private void getKoordinate() {
                System.out.println("Stačiakampio įstrižainių susikirtimo taško koordinatė: (X : " + (ilgis / 2) + " Y : " + (koordinate / 2) + ")"); }

        }
// Constructor
        Rectangle S1 = new Rectangle();

         // Attributes values
         S1.setIlgis(4);
         S1.setPlotis(7);
         S1.setKoordinateY(8);

         // execute methods
        S1.getPlotas();
        S1.getPerimetras();
        S1.getIstrizaine();
        S1.getKoordinate();

        System.out.println();

        Rectangle S2 = new Rectangle();
        S2.getIlgis();
        S2.getPlotis();
        S2.getKoordinateY();
        S2.getPlotas();
        S2.getPerimetras();
        S2.getIstrizaine();
        S2.getKoordinate();
    }
}
